            <Label text="Fav tool">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Label>
            <JFXRadioButton fx:id="csvToggle" text="Angular">
               <VBox.margin>
                  <Insets top="10.0" />
